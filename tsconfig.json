{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@/*": ["./*"],
      "components/*": ["src/components/*"],
      "screens/*": ["src/screens/*"],
      "hooks/*": ["src/hooks/*"],
      "assets/*": ["assets/*"],
      "context/*": ["src/context/*"],
      "services/*": ["src/services/*"],
      "utils/*": ["src/utils/*"],
      "navigation/*": ["src/navigation/*"]
    }
  }
}
